@page
@model FUNewsManagementWebRazorPage.Pages.Tags.CreateModel
@{
    ViewData["Title"] = "Create Tag";
}

<h1>Create Tag</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Tag.TagId"></label>
        <input asp-for="Tag.TagId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Tag.TagName"></label>
        <input asp-for="Tag.TagName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Tag.Note"></label>
        <input asp-for="Tag.Note" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Create</button>
    <a asp-page="Index" class="btn btn-secondary">Cancel</a>
</form>
